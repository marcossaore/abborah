
datasource db {
  provider = "mysql"
  url      = env("DB_CONNECTION")
}

generator client {
  provider = "prisma-client-js"
}

model Project {
  id                       Int     @id @default(autoincrement())
  name                     String
  description              String?
  startDate                DateTime
  endDate                  DateTime
  finished                 Boolean? @default(false)
}